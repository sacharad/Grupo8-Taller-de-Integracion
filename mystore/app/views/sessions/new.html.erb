

<div Class="tit">LOG IN</div>
<div class="elegant-aero">

<%= form_tag sessions_path do %>
  <label>
    <span><%= label_tag :login, "Username o Email" %></span>
    <%= text_field_tag :login, params[:login] %>
  </label>
</br>
  <label>
    <span><%= label_tag :password %></span>
    <%= password_field_tag :password %>
  </label>
  <p class="button">
  	<%= submit_tag(value="Ingresar") %></p>
<% end %>
</div>