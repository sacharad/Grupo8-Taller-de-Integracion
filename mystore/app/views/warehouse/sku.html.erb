<div class="row">
  <h1>Almacen: <%= @almacen_id %></h1>
  <h3>Sku: <%= @sku_id %></h3>
</div>
<div class="row">
  <div class="col-md-10 col-md-offset-1">
  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th>Almacen ID</th>
        <th>Sku ID</th>
        <th>ID</th>
        <th>Direccion</th>
        <th>Precio</th>
        <th>Despachado</th>
        <th>Costo</th>
      </tr>
    </thead>
    <tbody>
      <% @sku_info.each do |s_info|%>
        <tr>
          <td><%= @almacen_id %></td>
          <td><%= @sku_id %></td>
          <td><%= s_info["_id"] %></td>
          <td><%= s_info["direccion"] %></td>
          <td><%= s_info["precio"] %></td>
          <td><%= s_info["despachado"] %></td>
          <td><%= s_info["costo"] %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  </div>
</div>