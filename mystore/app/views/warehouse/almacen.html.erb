<div class="row">
  <h1>Almacen: <%= @almacen_id %></h1>
  <h3>Skus:</h3>
</div>
<div class="row">
  <div class="col-md-10 col-md-offset-1">
  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th>Sku ID</th>
        <th>Total</th>
        <th>Sku</th>
      </tr>
    </thead>
    <tbody>
      <% @skus.each do |sku|%>
        <tr>
          <td><%= sku["_id"] %></td>
          <td><%= sku["total"] %></td>
          <td><a class="btn btn-default" href="<%= sku_path(@almacen_id, sku["_id"]) %>">Ver Sku</a></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  </div>
</div>